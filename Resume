 <script>
  var canvas = document.getElementById("myCanvas");
  var ctx = canvas.getContext("2d");

  var data = [70, 100, 75];

  var barWidth = 70;
  var barSpacing = 30;
  var startX = 60;

  var maxValue = Math.max(...data);

  for (var i = 0; i < data.length; i++) {
    var barHeight = (data[i] / maxValue) * (canvas.height - 50);
    var x = startX + (barWidth + barSpacing) * i;
    var y = canvas.height - barHeight - 20;
    
    ctx.fillStyle = "#3399FF";
    ctx.fillRect(x, y, barWidth, barHeight);

    ctx.fillStyle = "white";
    ctx.fillText(data[i], x + barWidth / 2 - 10, y - 5);

    ctx.fillStyle = "white";
    ctx.fillText("Teamwork", 70, 290); 

    ctx.fillStyle = "white";
    ctx.fillText("Communication", 160, 290);

    ctx.fillStyle = "white";
    ctx.fillText("Leadership", 270, 290); 
  }
  </script>
  
